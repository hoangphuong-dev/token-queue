<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_his_patient" model="ir.sequence">
            <field name="name">Clinic Patient</field>
            <field name="code">his.patient</field>
            <field name="prefix">HIS</field>
            <field name="padding">3</field>
            <field name="clinic_auto_create" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_clinic_physician" model="ir.sequence">
            <field name="name">Clinic Physician</field>
            <field name="code">his.physician</field>
            <field name="prefix">DR</field>
            <field name="padding">3</field>
            <field name="clinic_auto_create" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
